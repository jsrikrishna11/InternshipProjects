<html>
<head>
    <%include ../partials/head.ejs %>
</head>
<body>

    <% include ../partials/constant.ejs %>
    <div>
        <br>
    </div>
    <form action="/change" method="POST">
    <table class="table table-inverse" style="padding: 40px;">
        <thead>
          <% for(var i=0; i< keys.length; i++){ %>
                <th><%=keys[i]%></th>
          <%}%>
        
        </thead>
        
         <tbody>
           <form action="/change" method="post">
          <% for(var i=0; i < data.length; i++) { %>
         <tr>
           <%for (var j=0; j< keys.length; j++){ %>
            <td> 
              <%= data[i][keys[j]] %>
              <%if( j == 0){ %>
              
              <input type="radio" name="employeeID" value="<%= data[i][keys[j]] %>">
            <%}%>
            </td>
           <%}%>
           
        </tr>
          
      <% } %>
    
        </tbody> 
        
      </table>
      <input type="submit" value="Edit">
      </form>
</body>
</html>
